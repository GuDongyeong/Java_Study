package java06_class.method.quiz;

public class Calculator {//기능 클래스

	public int sum(int n1, int n2 ) {	
		
		return n1 + n2;	
	}
	
	
	public int sub(int n1, int n2 ) {	
		
		return n1 - n2;	
	}
	
	
	public int mul(int n1, int n2 ) {	
		
		return n1 * n2;
	}
	
	
	public double div(int n1, int n2 ) {	
		
		return n1 / (double)n2;	
	}
	
}
