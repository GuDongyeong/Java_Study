package java06_class.method;

public class Method_03 {//기능 클래스
	
	public void arrTest01(int[] arr) {//int[]배열, 참조형
		arr[2] = 888;
	}
	
	public void arrTest02(int num) {//int, 기본형
		num = 999;
	}
	
	//-------------------------------------------
	
	public int arrTest03() {
		int num =10;
		
		return num;
		
	}
	
	public int[] arrTest04() {
		int[] arr = new int[3];
		
		return arr;
		
	}

}
