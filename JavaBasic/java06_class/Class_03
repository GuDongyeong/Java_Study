package java06_class;

public class Class_03 {
	
	private int num;//번호
	private String name; // 이름
	private double height; //키
	
	//공개하고 싶은 메소드
	
	//Getter Method - num필드의 값을 알려준다(반환한다)
	public int getNum() {
		return num;
	}
	
	//Setter Method - num 필드에 값을 저장한다
	public void setNum(int num) {
		this.num = num;
	}

}
